   "source": [
    "class Node:\n",
    "    def __init__(self, value):\n",
    "        self.value = value\n",
    "        self.next = None\n",
    "\n",
    "def create_circular_reference():\n",
    "    node1 = Node(1)\n",
    "    node2 = Node(2)\n",
    "    node1.next = node2\n",
    "    node2.next = node1  # 创建循环引用\n",
    "\n",
    "create_circular_reference()\n"
   ]
  }
